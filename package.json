{
	"name": "obsidian-plugin-journal-review",
	"version": "3.0.1",
	"description": "Review your daily notes on their anniversaries, like \"what happened today last year\"\n\n",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && npm run format && git add manifest.json versions.json",
		"lint": "eslint . && knip && prettier --check .",
		"format": "prettier --write .",
		"generateNotes": "ts-node ./scripts/generateNotes.ts vault 5",
		"knip": "knip"
	},
	"keywords": [],
	"author": "Kageetai",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"@types/node": "^25.2.2",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.3",
		"esbuild-plugin-copy": "^2.1.1",
		"eslint": "^10.0.0",
		"globals": "^17.3.0",
		"knip": "^5.83.1",
		"moment": "^2.30.1",
		"obsidian": "latest",
		"prettier": "^3.8.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0"
	},
	"dependencies": {
		"obsidian-daily-notes-interface": "^0.9.4",
		"preact": "^10.28.3"
	},
	"overrides": {
		"obsidian-daily-notes-interface": {
			"obsidian": "$obsidian"
		}
	}
}
