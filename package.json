		"builtin-modules": "^3.3.0",
		"esbuild": "^0.20.2",
		"esbuild-plugin-copy": "^2.1.1",
		"eslint": "^8.57.0",
		"obsidian": "latest",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2"
	},
